    "copy-to-clipboard": "^3.0.8",
    "electron-is-dev": "^0.3.0",
    "fs-jetpack": "^1.3.0",
    "qrcode": "^1.2.0",
    "react": "^16.3.2",
    "react-code-input": "^3.1.0",
    "react-dom": "^16.3.2",
    "semantic-ui": "^2.3.1",
    "speakeasy": "^2.0.0"
