{
  "name": "TronWallet",
  "productName": "Tron Wallet",
  "version": "1.0.0",
  "description": "Tron Wallet for decentralized internet",
  "main": "main.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "bundle:prod": "webpack --mode production",
    "serve": "electron .",
    "start:dev": "npm-run-all --parallel bundle serve",
    "start:prod": "npm-run-all --parallel bundle:prod serve",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=client/images/tronlogolight.icns --prune=true --out=releases",
    "package-win": "electron-packager . TronWallet --overwrite --platform=win32 --arch=ia32 --icon=client/images/tronlogolight.ico --prune=true --out=releases --version-string.CompanyName=Tron --version-string.FileDescription=Tron --version-string.ProductName=\"Tron Wallet\"",
    "mac-dmg": "electron-installer-dmg ./releases/Tron\\ Wallet-darwin-x64/Tron\\ Wallet.app Tron Wallet --out=releases --overwrite --icon=client/images/tronlogolight.icns  --background=./client/images/tronbluetransparent.png"
  },
  "repository": "https://gitlab.com/TronWallet/TronWalletDesktop.git",
  "keywords": [
    "Tron",
    "Wallet",
    "Internet",
    "Decentralized",
    "Justin",
    "Sun",
    "Love"
  ],
  "author": "Gurkirat",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-installer-dmg": "^1.0.0",
    "electron-packager": "^12.0.1",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glamor": "^2.20.40",
    "import-glob-loader": "^1.1.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webworkify-webpack": "^1.0.6"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "bson": "^2.0.6",
    "copy-to-clipboard": "^3.0.8",
    "css-font-family": "^1.0.7",
    "deep-equal": "^1.0.1",
    "dom-to-image": "^2.6.0",
    "egoroof-blowfish": "^2.1.0",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "file-saver": "^1.3.8",
    "fs": "0.0.1-security",
    "fs-jetpack": "^1.3.0",
    "hex-to-array-buffer": "^1.1.0",
    "instascan": "^1.0.0",
    "net": "^1.0.2",
    "particles.js": "^2.0.0",
    "qrcode": "^1.2.0",
    "react": "^16.3.2",
    "react-code-input": "^3.1.0",
    "react-dom": "^16.3.2",
    "react-simple-maps": "^0.12.1",
    "semantic-ui": "^2.3.1",
    "speakeasy": "^2.0.0",
    "stompjs": "0.0.3"
  }
}
