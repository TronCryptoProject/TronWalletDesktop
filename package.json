{
  "name": "TronWallet",
  "productName": "Tron Wallet",
  "version": "1.0.0",
  "description": "Tron Wallet for decentralized internet",
  "main": "main.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "bundle:prod": "webpack --mode production",
    "serve": "electron .",
    "start:dev": "npm-run-all --parallel bundle serve",
    "start:prod": "npm-run-all --parallel bundle:prod serve",
