  },
  "repository": "https://gitlab.com/TronWallet/TronWalletDesktop.git",
  "keywords": [
    "Tron",
    "Wallet",
    "Internet",
    "Decentralized",
    "Justin",
    "Sun",
    "Love"
  ],
  "author": "Gurkirat",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-packager": "^12.0.1",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glamor": "^2.20.40",
